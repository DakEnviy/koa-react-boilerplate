{
  "name": "react-starterkit",
  "version": "1.0.0",
  "description": "React starterkit with Koa",
  "author": "Alberto Gasparin",
  "private": true,
  "scripts": {
    "start": "node --harmony ./server.js",
    "start:watch": "nodemon --ignore public/ --delay 1 --ext js,jsx --harmony ./server.js",
    "build": "NODE_ENV=production webpack -p --progress --colors --display-error-details --display-modules --sort-modules-by name",
    "build:watch": "webpack --colors --display-error-details && webpack-dev-server --config webpack.config.js",
    "watch": "concurrent \"npm run build:watch -s\" \"npm run start:watch -s\"",
    "pretest": "npm run lint -s",
    "test": "mocha $(find . -name '*.spec.js' ! -path '*/node_modules/*') --compilers js:babel-register",
    "test:watch": "mocha $(find . -name '*.spec.js' ! -path '*/node_modules/*') --compilers js:babel-register --watch --watch-extensions jsx --reporter min",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore ."
  },
  "keywords": [],
  "dependencies": {
    "autoprefixer-loader": "^3.0.0",
    "babel-core": "^6.1.0",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.0",
    "babel-preset-react": "^6.1.0",
    "babel-preset-stage-2": "^6.1.0",
    "babel-register": "^6.2.0",
    "css-loader": "^0.23.0",
    "es6-promises": "^1.0.0",
    "extract-text-webpack-plugin": "^0.9.0",
    "file-loader": "^0.8.0",
    "history": "^1.13.0",
    "image-webpack-loader": "^1.6.2",
    "koa": "^1.1.0",
    "koa-bodyparser": "^2.0.0",
    "koa-compress": "^1.0.0",
    "koa-error": "^1.1.0",
    "koa-generic-session": "^1.9.0",
    "koa-jade": "^2.0.0",
    "koa-logger": "^1.3.0",
    "koa-proxy": "^0.4.0",
    "koa-router": "^5.1.0",
    "koa-static-cache": "^3.1.0",
    "lodash": "^3.10.0",
    "moment": "^2.10.0",
    "node-sass": "^3.4.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0",
    "sass-loader": "^3.1.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.0",
    "webpack": "^1.12.0",
    "whatwg-fetch": "^0.10.0"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "concurrently": "^1.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.6.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.0",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.2.0",
    "react-shallow-testutils": "^0.6.0",
    "sinon": "^1.17.0",
    "source-map-loader": "^0.1.0",
    "webpack-dev-server": "^1.14.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
